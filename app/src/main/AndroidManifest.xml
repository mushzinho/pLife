        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DATABASE"
            android:value="SUGAR_BLIFE_DB" />
        <meta-data
            android:name="VERSION"
            android:value="16" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />

        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>

        <activity
            android:name=".AdicionarDoadorActivity"
            android:label="@string/adicionar_doador_titulo">

        </activity>
        <activity
            android:name=".AdicionarDoacaoActivity"
            android:label="@string/adicionar_doacao_titulo">




        </activity>
        <activity android:name=".AdicionarSaidaActivity">

        </activity>

        <activity android:name=".AdicionarCampanhaActivity">



        </activity>
    </application>

</manifest>